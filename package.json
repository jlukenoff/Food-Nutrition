    "coverageDirectory": "<rootDir>/__test__/coverage/",
    "resolver": null,
    "testMatch": [
      "**/__test__/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlukenoff/Food-Nutrition.git"
  },
  "keywords": [],
