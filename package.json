  "bugs": {
    "url": "https://github.com/jlukenoff/Food-Nutrition/issues"
  },
  "homepage": "https://github.com/jlukenoff/Food-Nutrition#readme",
  "dependencies": {
    "express": "^4.16.3",
    "nodemon": "^1.18.4",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-paginate": "^5.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-airbnb": "^2.6.0",
