{
  "name": "Food-Nutrition",
  "version": "1.0.0",
  "description": "Food Nutriton API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node server/index.js",
    "start-dev": "nodemon --inspect server/index.js",
    "react-dev": "webpack --watch",
    "heroku-postbuild": "webpack -p"
